[run]
omit =
    src/models/categorias/almacenamiento.py
    src/models/composicion/comedor.py
    src/models/concretos/sofacama.py
    src/services/tienda.py

[report]
exclude_lines =
    pragma: no cover
    if __name__ == .__main__.:  # pragma: no cover
[run]
source = src
omit =
    */__pycache__/*
    */tests/*
    */venv/*
    src/ui/*
    src/main.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    pass

fail_under = 80
